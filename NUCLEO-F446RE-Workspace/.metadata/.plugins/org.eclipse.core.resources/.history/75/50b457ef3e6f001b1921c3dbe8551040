/********************************************************************
 * INCLUDES
 */
#include "led.h"
#include "uart.h"

/********************************************************************
 * EXPORT FUNCTIONS
 */
void initLED(void)
{
	  HAL_GPIO_WritePin(GPIOA, GPIO_PIN_5, GPIO_PIN_SET);
}
void setLED(void)
{
	printString("setLED\r\n");
}

void resetLED(void)
{
	printString("resetLED\r\n");
}

